<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="Student" class="com.Spring_IOC_dependency_Injection.Dependancy_Injection_demo.Student">
<property name="studentName" value="Dinesh Kumar"></property>
<property name="id" value="1"></property>
</bean>

<bean id="Ashish" class="com.Spring_IOC_dependency_Injection.Dependancy_Injection_demo.Student">
<property name="studentName" value="Ashish Kumar"></property>
<property name="id" value="2"></property>
</bean>
</beans>